<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<TextView android:id="@+id/tvSession" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginRight="6px"/>
		<TextView android:id="@+id/tvRowNumber" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginRight="6px"/>
		<TextView android:id="@+id/teRowTextExpression"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
	</LinearLayout>

	<TextView android:id="@+id/teRowTextValue"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />
</LinearLayout>